                        <div class="navbar-nav ml-auto py-0">
                            <a href="{{ path('activity_public_index') }}" class="nav-item nav-link active">Home</a>
                            <a href="{{ path('activity_public_index') }}" class="nav-item nav-link">Activities</a>
                            <a href="{{ path('activity_public_index') }}" class="nav-item nav-link">Events</a>
